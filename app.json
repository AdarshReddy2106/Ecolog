{
  "expo": {
    "name": "ECOLOG",
    "slug": "ecolog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tree-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/tree-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#d8e8d2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tree-icon.png",
        "backgroundColor": "#d8e8d2"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tree-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d8e8d2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}

