{
  "expo": {
    "name": "ECOLOG",
    "slug": "ecolog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tree-icon.webp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/tree-icon.webp",
      "resizeMode": "contain",
      "backgroundColor": "#d8e8d2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adarsh_21.treeiq"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tree-icon.webp",
        "backgroundColor": "#d8e8d2"
      },
      "package": "com.adarsh_21.treeiq"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tree-icon.webp",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d8e8d2"
        }
      ],
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "482a6c94-5248-4246-a552-0bbb51e4c24e"
      }
    },
    "developmentClient": {
      "silentLaunch": true
    }
  }
}
