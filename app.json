{
  "expo": {
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "name": "TreeIQ",
    "slug": "treeiq",
    "orientation": "portrait",
    "icon": "./assets/images/tree-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/tree-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#d8e8d2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tree-icon.png",
        "backgroundColor": "#d8e8d2"
      },
      "package": "com.adarsh_21.treeiq"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tree-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d8e8d2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "482a6c94-5248-4246-a552-0bbb51e4c24e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/482a6c94-5248-4246-a552-0bbb51e4c24e"
    }
  }
}